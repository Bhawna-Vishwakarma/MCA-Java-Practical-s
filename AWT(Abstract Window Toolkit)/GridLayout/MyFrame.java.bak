import java.awt.*; 
class MyFrame extends Frame 
{ 
	MyFrame() 
	{ 
		this.setVisible(true); 
		this.setSize(500,500); 
		this.setTitle("GridLayout"); 
		this.setBackground(Color.red); 
		this.setLayout(new GridLayout(4,4)); 
		for (int i=1;i<=10 ;i++ ) 
		{ 
			Button b=new Button(""+i); 
			this.add(b); 
		} 
	}
}